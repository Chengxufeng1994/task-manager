    "mongodb": "^4.0.1",
    "mongoose": "^5.13.3",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "eslint": "7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
